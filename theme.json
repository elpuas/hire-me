{
    "version": 2,
    "settings": {
      "color": {
        "palette": [
          {
            "slug": "background",
            "color": "#ffffff",
            "name": "Background"
          },
          {
            "slug": "primary",
            "color": "#0073aa",
            "name": "Primary"
          },
          {
            "slug": "secondary",
            "color": "#444",
            "name": "Secondary"
          }
        ]
      },
      "typography": {
        "fontSizes": [
          {
            "slug": "small",
            "size": "12px",
            "name": "Small"
          },
          {
            "slug": "normal",
            "size": "16px",
            "name": "Normal"
          },
          {
            "slug": "large",
            "size": "24px",
            "name": "Large"
          }
        ],
        "fontFamilies": [
          {
            "slug": "system",
            "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
            "name": "System"
          }
        ]
      },
      "spacing": {
        "customPadding": true,
        "customMargin": true,
        "units": ["px", "em", "rem", "vh", "vw"]
      },
      "blocks": {
        "core/heading": {
          "fontSize": "normal",
          "color": {
            "text": "var:preset|color|secondary"
          }
        },
        "core/paragraph": {
          "fontSize": "normal",
          "color": {
            "text": "var:preset|color|primary"
          }
        }
      },
      "layout": {
        "contentSize": "1220px",
        "wideSize": "1440px"
        }
    },
    "styles": {
      "elements": {
        "link": {
          "color": {
            "text": "var:preset|color|primary",
            "hover": "var:preset|color|secondary"
          }
        }
      },
      "blocks": {
        "core/heading": {
          "typography": {
            "fontSize": "var:preset|font-size|large"
          },
          "color": {
            "text": "var:preset|color|primary"
          }
        },
        "core/paragraph": {
          "typography": {
            "fontSize": "var:preset|font-size|normal"
          },
          "color": {
            "text": "var:preset|color|secondary"
          }
        }
      }
    }
  }
